<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.netvlab.model.Submitedtopo" table="submitedtopo" schema="vnetwork">
        <id name="id" column="Id"/>
        <property name="userId" column="UserID"/>
        <property name="networksJson" column="NetworksJSON"/>
        <property name="subnetsJson" column="SubnetsJSON"/>
        <property name="serversJson" column="ServersJSON"/>
        <property name="submitTime" column="submitTime"/>
    </class>
</hibernate-mapping>